<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sjxy.graduate.dao.ScenicDao">

    <resultMap type="cn.sjxy.graduate.entity.Scenic" id="ScenicMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="icon" column="icon" jdbcType="VARCHAR"/>
        <result property="img" column="img" jdbcType="VARCHAR"/>
        <result property="area" column="area" jdbcType="VARCHAR"/>
        <result property="ticket" column="ticket" jdbcType="VARCHAR"/>
        <result property="desc" column="desc" jdbcType="VARCHAR"/>
        <result property="typeId" column="type_id" jdbcType="INTEGER"/>
        <result property="evaluate" column="evaluate" jdbcType="VARCHAR"/>
        <result property="hot" column="hot" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="queryById" resultMap="ScenicMap">
    select * from scenic where id=#{id}
</select>

</mapper>